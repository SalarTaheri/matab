<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans-2.0.dtd">
<beans default-lazy-init="false" default-autowire="no" 
	default-dependency-check="none"> 
	<!-- AccountBookService -->
	<bean id="accountBookService" class="com.artonis.fnc.reports.books.service.impl.AccountBookServiceImpl"> 
		<property name="dao" ref="accountBookDao" /> 
	</bean>
	<bean id="accountBookDao" class="com.artonis.fnc.reports.books.dao.impl.AccountBookDaoImpl"> 
		<property name="sessionFactory" ref="sessionFactory" />
	</bean> 
	<!-- FloatAccountBookService -->
	<bean id="floatAccountBookService" class="com.artonis.fnc.reports.books.service.impl.FloatAccountBookServiceImpl"> 
		<property name="dao" ref="floatAccountBookDao" /> 
	</bean>
	<bean id="floatAccountBookDao" class="com.artonis.fnc.reports.books.dao.impl.FloatAccountBookDaoImpl"> 
		<property name="sessionFactory" ref="sessionFactory" />
	</bean> 
</beans>