<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans-2.0.dtd">
<beans default-lazy-init="false" default-autowire="no" 
	default-dependency-check="none"> 
	<!-- TestBalanceService -->
	<bean id="testBalanceService" class="com.artonis.fnc.reports.balance.service.impl.TestBalanceServiceImpl"> 
		<property name="dao" ref="testBalanceDao" /> 
	</bean>
	<bean id="testBalanceDao" class="com.artonis.fnc.reports.balance.dao.impl.TestBalanceDaoImpl"> 
		<property name="sessionFactory" ref="sessionFactory" />
	</bean> 
	<!-- FloatAccountBalanceService -->
	<bean id="floatAccountBalanceService" class="com.artonis.fnc.reports.balance.service.impl.FloatAccountBalanceServiceImpl"> 
		<property name="dao" ref="floatAccountBalanceDao" /> 
	</bean>
	<bean id="floatAccountBalanceDao" class="com.artonis.fnc.reports.balance.dao.impl.FloatAccountBalanceDaoImpl"> 
		<property name="sessionFactory" ref="sessionFactory" />
	</bean> 
	<!-- ReviewAccountService -->
	<bean id="reviewAccountService" class="com.artonis.fnc.reports.balance.service.impl.ReviewAccountServiceImpl"> 
		<property name="dao" ref="reviewAccountDao" /> 
	</bean>
	<bean id="reviewAccountDao" class="com.artonis.fnc.reports.balance.dao.impl.ReviewAccountDaoImpl"> 
		<property name="sessionFactory" ref="sessionFactory" />
	</bean> 
</beans>