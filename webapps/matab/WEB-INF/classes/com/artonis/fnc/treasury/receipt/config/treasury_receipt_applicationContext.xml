		<property name="sessionFactory" ref="sessionFactory" />
	</bean> 
	<!-- ReceiptBillBankingBillService -->
	<bean id="receiptBillBankingBillService" class="com.artonis.fnc.treasury.receipt.service.impl.ReceiptBillBankingBillServiceImpl"> 
		<property name="dao" ref="receiptBillBankingBillDao" /> 
	</bean>
	<bean id="receiptBillBankingBillDao" class="com.artonis.fnc.treasury.receipt.dao.impl.ReceiptBillBankingBillDaoImpl"> 
		<property name="sessionFactory" ref="sessionFactory" />
