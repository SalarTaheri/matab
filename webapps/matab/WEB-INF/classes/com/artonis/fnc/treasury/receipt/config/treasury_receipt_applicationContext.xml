<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans-2.0.dtd">
<beans default-lazy-init="false" default-autowire="no" 
	default-dependency-check="none"> 
	<!-- ReceiptBillService -->
	<bean id="receiptBillService" class="com.artonis.fnc.treasury.receipt.service.impl.ReceiptBillServiceImpl"> 
		<property name="dao" ref="receiptBillDao" /> 
	</bean>
	<bean id="receiptBillDao" class="com.artonis.fnc.treasury.receipt.dao.impl.ReceiptBillDaoImpl"> 
		<property name="sessionFactory" ref="sessionFactory" />
	</bean> 
	<!-- ReceiptBillPayerService -->
	<bean id="receiptBillPayerService" class="com.artonis.fnc.treasury.receipt.service.impl.ReceiptBillPayerServiceImpl"> 
		<property name="dao" ref="receiptBillPayerDao" /> 
	</bean>
	<bean id="receiptBillPayerDao" class="com.artonis.fnc.treasury.receipt.dao.impl.ReceiptBillPayerDaoImpl"> 
		<property name="sessionFactory" ref="sessionFactory" />
	</bean> 
	<!-- ReceiptBillCashReceiptService -->
	<bean id="receiptBillCashReceiptService" class="com.artonis.fnc.treasury.receipt.service.impl.ReceiptBillCashReceiptServiceImpl"> 
		<property name="dao" ref="receiptBillCashReceiptDao" /> 
	</bean>
	<bean id="receiptBillCashReceiptDao" class="com.artonis.fnc.treasury.receipt.dao.impl.ReceiptBillCashReceiptDaoImpl"> 
		<property name="sessionFactory" ref="sessionFactory" />
