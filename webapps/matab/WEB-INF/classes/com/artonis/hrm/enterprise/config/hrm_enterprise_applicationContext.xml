<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN//EN" "http://www.springframework.org/dtd/spring-beans-2.0.dtd">
<beans default-lazy-init="false" default-autowire="no"
	default-dependency-check="none"><!-- DAO -->
	<!-- InvoiceService -->

	<bean id="invoiceService" class="com.artonis.hrm.enterprise.service.impl.InvoiceServiceImpl">
		<property name="dao" ref="invoiceDao" />
	</bean>
	<bean id="invoiceDao" class="com.artonis.hrm.enterprise.dao.impl.InvoiceDaoImpl">
		<property name="sessionFactory" ref="sessionFactory" />
	</bean>
	<!-- JobService -->
	<bean id="jobService" class="com.artonis.hrm.enterprise.service.impl.JobServiceImpl"> 
		<property name="dao" ref="jobDao" /> 
	</bean>
	<bean id="jobDao" class="com.artonis.hrm.enterprise.dao.impl.JobDaoImpl"> 
		<property name="sessionFactory" ref="sessionFactory" />
	</bean> 
	<!-- PositionService -->
	<bean id="positionService" class="com.artonis.hrm.enterprise.service.impl.PositionServiceImpl"> 
		<property name="dao" ref="positionDao" /> 
	</bean>
	<bean id="positionDao" class="com.artonis.hrm.enterprise.dao.impl.PositionDaoImpl"> 
		<property name="sessionFactory" ref="sessionFactory" />
	</bean> 
</beans>


