<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.artonis.lab.operation.view" default-lazy="false">
	<class name="LabQueueView" table="lab2labqueue_view">
		<id name="id" type="long">
			<generator class="native"/>
		</id>
		<property name="patientId" column="patient_id" type="long"/>
		<property name="doctorId" column="doctor_id" type="long"/>
		<property name="medicalHistory" column="medicalhistory" type="string"/>
		<property name="expirationDate" column="expirationdate" type="com.artonis.core.base.core.Date"/>
		<property name="insuranceOrganizationId" column="insurance_organization_id" type="long"/>
		<property name="insuranceOrganizationName" column="insurance_organization_name" type="string"/>
		<property name="bookPage" column="bookpage" type="int"/>
		<property name="bookNumber" column="booknumber" type="string"/>
		<property name="lmp" column="lmp" type="com.artonis.core.base.core.Date"/>
		<property name="specialtyId" column="specialty_id" type="long"/>
		<property name="factorId" column="factor_id" type="long"/>
		<property name="receptionNumber" column="reception_number" type="int"/>
		<property name="date" column="date" type="com.artonis.core.base.core.Date"/>
		<property name="canChange" column="can_change" type="boolean"/>
		<property name="printCount" column="print_count" type="int"/>
		<property name="firstName" column="firstName" type="string"/>
		<property name="lastName" column="lastName" type="string"/>
		<property name="fileNumber" column="fileNumber" type="string"/>
		<property name="doctorName" column="doctor_name" type="string"/>
		<property name="age" column="age" type="int"/>
		<property name="sex" column="sex" type="int"/>
		<property name="birthYear" column="birthYear" type="int"/>
		<property name="debt" column="debt" type="long"/>
	</class>
	<class name="TestResultView" table="lab2test_result_view">
		<id name="id" type="long">
			<generator class="native"/>
		</id>
		<property name="labQueueId" column="labqueue_id" type="long"/>
		<property name="testId" column="test_id" type="long"/>
		<property name="result" column="result" type="string"/>
		<property name="date" column="date" type="com.artonis.core.base.core.Date"/>
		<property name="normalRangeMin" column="normal_range_min" type="double"/>
		<property name="normalRangeMax" column="normal_range_max" type="double"/>
		<property name="warningRangeMin" column="warning_range_min" type="double"/>
		<property name="warningRangeMax" column="warning_range_max" type="double"/>
		<property name="possibleRangeMin" column="possible_range_min" type="double"/>
		<property name="possibleRangeMax" column="possible_range_max" type="double"/>
		<property name="measurementUnitId" column="measurement_unit_id" type="long"/>
		<property name="measurementUnitName" column="measurement_unit_name" type="string"/>
		<property name="descriptions" column="descriptions" type="string"/>
		<property name="testCode" column="test_code" type="string"/>
		<property name="testName" column="test_name" type="string"/>
		<property name="testPrintOrder" column="test_print_order" type="int"/>
		<property name="unitId" column="unit_id" type="long"/>
		<property name="unitName" column="unit_name" type="string"/>
		<property name="unitPrintOrder" column="unit_print_order" type="int"/>
	</class>
	<class name="LabRegistrationView" table="lab2labRegistration_view">
		<id name="id" type="long">
			<generator class="native"/>
		</id>
		<property name="patientId" column="patient_id" type="long"/>
		<property name="doctorId" column="doctor_id" type="long"/>
		<property name="medicalHistory" column="medicalhistory" type="string"/>
		<property name="insuranceOrganizationId" column="insurance_organization_id" type="long"/>
		<property name="insuranceOrganizationName" column="insurance_organization_name" type="string"/>
		<property name="lmp" column="lmp" type="com.artonis.core.base.core.Date"/>
		<property name="factorId" column="factor_id" type="long"/>
		<property name="receptionNumber" column="reception_number" type="int"/>
		<property name="date" column="date" type="com.artonis.core.base.core.Date"/>
		<property name="firstName" column="firstName" type="string"/>
		<property name="lastName" column="lastName" type="string"/>
		<property name="doctorName" column="doctor_name" type="string"/>
		<property name="doctorPrefix" column="doctor_prefix" type="string"/>
		<property name="age" column="age" type="int"/>
		<property name="sex" column="sex" type="int"/>
		<property name="birthYear" column="birthYear" type="int"/>
		<property name="cellphone" column="cellphone" type="string"/>
		<property name="payable" column="payable" type="long"/>
		<property name="insuranceAmount" column="insurance_amount" type="long"/>
		<property name="received" column="received" type="long"/>
		<property name="responseDate" type="com.artonis.core.base.core.Date"/>
	</class>
	<class name="TestResultLabQueueView" table="lab2test_result_labqueue_view">
		<id name="id" type="long">
			<generator class="native"/>
		</id>
		<property name="labQueueId" column="labqueue_id" type="long"/>
		<property name="testId" column="test_id" type="long"/>
		<property name="labQueueDate" column="labqueue_date" type="com.artonis.core.base.core.Date"/>
		<property name="receptionNumber" column="reception_number" type="int"/>
	</class>
	<class name="TaskListGroupByTest" table="lab1test">
		<id name="id" type="long">
			<generator class="native"/>
		</id>
		<property name="abbreviatedName" column="abbreviated_name" type="string"/>
		<property name="unitId" column="unit_id" type="long"/>
		<set name="labQueues" table="lab2test_result_labqueue_view" inverse="true" lazy="false" fetch="select">
			<key>
				<column name="test_id" not-null="true"/>
			</key>
			<one-to-many class="com.artonis.lab.operation.view.TestResultLabQueueView"/>
		</set>
	</class>
	<class name="LabQueueParsipolView" table="lab2labqueue_parsipol_view">
		<id name="id" type="long">
			<generator class="native"/>
		</id>
		<property name="patientId" column="patient_id" type="long"/>
		<property column="parsipol_doctor_id" name="doctorId" type="int"/>
		<property column="doctor_name" name="doctorName" type="string"/>
		<property column="doctor_specialization" name="specialization" type="string"/>
		<property column="insurance_num" name="insuranceNumber" type="string"/>
		<property column="insurance_contract_id" name="insuranceContractId" type="int"/>
		<property column="insurance_contract_name" name="insuranceContractName" type="string"/>
		<property column="supp_insurance_contract_id" name="suppInsuranceContractId" type="int"/>
		<property column="supp_insurance_contract_name" name="suppInsuranceContractName" type="string"/>
		<property column="register_date" name="registerDate" type="com.artonis.core.base.core.Date"/>
		<property column="subscription_number" name="subscriptionNumber" type="string"/>
		<property column="pre_admit_id" name="preAdmitId" type="int"/>

		<property column="firstName" name="firstName" type="string"/>
		<property column="lastName" name="lastName" type="string"/>
		<property column="patientFullName" name="patientFullName" type="string"/>
		<property column="fileNumber" name="fileNumber" type="string"/>
		<property column="nationalcode" name="nationalCode" type="string"/>
		<property column="sex" name="sex" type="int"/>
		<property column="factor_id" name="factor_id" type="long"/>

	</class>
</hibernate-mapping>
